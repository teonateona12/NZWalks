<h1 class="mt-3">Regions</h1>

<table class="table table-border">
    <thead>
    <th>Id</th>
    <th>Code</th>
    <th>Name</th>
    </thead>
    <tbody>
        @foreach (var walk in Model)
        {
            <tr>
                <td>@walk.Id</td>
                <td>@walk.Code</td>
                <td>@walk.Name</td>
            </tr>
        }
    </tbody>
</table>